<%= simple_form_for(@meetup_group) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :owner, label_method: 'to_s' %>
    <%= f.input :topic %>
    <%= f.input :incepted_at %>
    <%= f.input :home_town %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
