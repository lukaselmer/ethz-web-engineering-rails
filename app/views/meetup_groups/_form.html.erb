<%= simple_form_for(@meetup_group) do |f| %>
  <%= f.error_notification %>

  <%= f.hidden_field :owner_id %>

  <div class="form-inputs">
    <%= f.input :topic %>
    <%= f.input :incepted_at %>
    <%= f.input :home_town %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-primary' %>
  </div>
<% end %>
